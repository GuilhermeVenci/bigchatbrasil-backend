{
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:prettier/recommended",
    "@rocketseat/eslint-config/node"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "rules": {
    "no-useless-constructor": "off",
    "prettier/prettier": "off",
    "no-extra-semi": "warn"
  }
}
